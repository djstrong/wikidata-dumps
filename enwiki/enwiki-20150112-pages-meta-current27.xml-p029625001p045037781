  | governing_body = [[National Park Service]]&lt;ref name=BYNUM-NPS&gt;{{Cite web|title=Bynum Mounds-National Register of Historic Places Indian Mounds of Mississippi Travel Itinerary|publisher=National Park Service|url=http://www.nps.gov/nr/travel/mounds/byn.htm|accessdate=2010-11-16}}&lt;/ref&gt;
  | refnum = 89000783&lt;ref name=&quot;nris&quot;/&gt;
